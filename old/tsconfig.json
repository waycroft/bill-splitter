{
    "compilerOptions": {
      "module": "commonjs",
      "noImplicitAny": false,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "target": "ES6",
      "esModuleInterop": true,
      "strictNullChecks": true,
      "noImplicitReturns": true
    },
    "include": ["./**/*"],
    "exclude": ["node_modules/**/*"]
}